$[language java]
$[domain Service]
$[author to org.entitycompiler.springboot.service]
    $[author to outlet methods]
        $[if entity.space.hasDomain("DocumentBuilder")]
            $[import "documentBuilder/MarkdownDocumentBuilderFunctions"]
            $[import "util/AutoWired"]
            $[call buildMarkdownDocSection(entity: entity)->(domainEntitiesToAutowire: domainEntitiesToAutowire)]
            $[foreach item in domainEntitiesToAutowire]
                $[call addAutoWired(domainEntity: item)]
            $[/foreach]
        $[/if]
    $[/author]
$[/author]